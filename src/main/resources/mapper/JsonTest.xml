<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuruichao.connection.pool.test.jsontest.JsonTestMapper">

    <insert id="insert" parameterType="java.util.List">
        INSERT INTO json_test1(name, first_name, last_name) VALUES
        <foreach collection="list" item="item" separator="," index="index">
            (#{item.name}, #{item.firstName}, #{item.lastName})
        </foreach>
    </insert>

    <insert id="insert2" parameterType="java.util.List">
        INSERT INTO json_test2(name) VALUES
        <foreach collection="list" item="item" separator="," index="index">
            (#{item.name})
        </foreach>
    </insert>
</mapper>

